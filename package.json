{
  "name": "starter-vite-orpc",
  "version": "0.0.0",
  "description": "A starter kit for a Vite + oRPC",
  "type": "module",
  "workspaces": [
    "./apps/*",
    "./packages/*"
  ],
  "scripts": {
    "auth:generate": "bun run ./scripts/auth-generate.ts",
    "db:generate": "bun run ./scripts/db-generate.ts",
    "dev": "alchemy dev",
    "format": "prettier --write .",
    "lint": "oxlint",
    "test": "vitest",
    "typecheck": "tsc --build"
  },
  "devDependencies": {
    "@better-auth/cli": "catalog:better-auth",
    "@cloudflare/workers-types": "catalog:cloudflare",
    "@types/bun": "^1.3.4",
    "alchemy": "^0.81.3",
    "dotenv": "^17.2.3",
    "drizzle-kit": "catalog:drizzle",
    "oxlint": "^1.33.0",
    "prettier": "^3.7.4",
    "typescript": "catalog:default",
    "vitest": "catalog:vitest"
  },
  "packageManager": "bun@1.3.4",
  "catalogs": {
    "default": {
      "zod": "^4.2.1",
      "typescript": "^5.9.3",
      "@types/node": "^24.10.0"
    },
    "vitest": {
      "vitest": "^4.0.16",
      "@vitest/browser-playwright": "^4.0.16",
      "vitest-browser-react": "^2.0.2"
    },
    "react": {
      "react": "^19.2.0",
      "react-dom": "^19.2.0",
      "@types/react": "^19.2.2",
      "@types/react-dom": "^19.2.2"
    },
    "tanstack": {
      "@tanstack/react-query": "^5.90.12",
      "@tanstack/react-router": "^1.135.0",
      "@tanstack/react-router-devtools": "^1.135.0",
      "@tanstack/router-plugin": "^1.135.0",
      "@tanstack/react-devtools": "^0.8.1"
    },
    "better-auth": {
      "better-auth": "^1.4.7",
      "@better-auth/cli": "^1.4.7"
    },
    "cloudflare": {
      "@cloudflare/workers-types": "^4.20251216.0"
    },
    "drizzle": {
      "drizzle-kit": "^0.31.8",
      "drizzle-orm": "^0.45.1",
      "drizzle-zod": "^0.8.3"
    },
    "orpc": {
      "@orpc/client": "^1.12.3",
      "@orpc/contract": "^1.12.3",
      "@orpc/openapi": "^1.12.3",
      "@orpc/openapi-client": "^1.12.3",
      "@orpc/server": "^1.12.3",
      "@orpc/tanstack-query": "^1.12.3"
    }
  }
}
