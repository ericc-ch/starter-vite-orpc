{
  "name": "starter-vite-orpc",
  "version": "0.0.0",
  "description": "A starter kit for a Vite + oRPC",
  "type": "module",
  "workspaces": [
    "./apps/*",
    "./packages/*"
  ],
  "scripts": {
    "auth:generate": "bun run ./scripts/auth-generate.ts",
    "build": "turbo run build",
    "db:generate": "bun run ./scripts/db-generate.ts",
    "deploy": "alchemy deploy",
    "dev": "alchemy dev",
    "format": "prettier --write .",
    "start": "turbo run start",
    "typecheck": "tsc --build"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.4.7",
    "@cloudflare/workers-types": "^4.20251216.0",
    "@types/bun": "^1.3.4",
    "alchemy": "^0.81.3",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "oxlint": "^1.33.0",
    "prettier": "^3.7.4",
    "turbo": "^2.6.3",
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.3.4",
  "catalogs": {
    "default": {
      "zod": "^4.2.1"
    },
    "better-auth": {
      "better-auth": "^1.4.7"
    },
    "cloudflare": {
      "@cloudflare/workers-types": "^4.20251216.0"
    },
    "drizzle": {
      "drizzle-kit": "^0.31.8",
      "drizzle-orm": "^0.45.1",
      "drizzle-zod": "^0.8.3"
    },
    "orpc": {
      "@orpc/client": "^1.12.3",
      "@orpc/contract": "^1.12.3",
      "@orpc/openapi": "^1.12.3",
      "@orpc/openapi-client": "^1.12.3",
      "@orpc/server": "^1.12.3"
    }
  }
}