{
  "name": "starter-vite-orpc",
  "version": "0.0.0",
  "description": "A starter kit for a Vite + oRPC",
  "type": "module",
  "workspaces": [
    "./apps/*",
    "./packages/*"
  ],
  "scripts": {
    "auth:generate": "bun run ./scripts/auth-generate.ts",
    "build": "turbo run build",
    "db:generate": "bun run ./scripts/db-generate.ts",
    "deploy": "alchemy deploy",
    "dev": "alchemy dev",
    "format": "prettier --write .",
    "start": "turbo run start",
    "typecheck": "tsc --build"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.34",
    "@cloudflare/workers-types": "^4.20251109.0",
    "@types/bun": "^1.3.2",
    "alchemy": "^0.77.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.6",
    "prettier": "^3.6.2",
    "turbo": "^2.6.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.3.2",
  "catalogs": {
    "default": {
      "zod": "^4.1.12"
    },
    "better-auth": {
      "better-auth": "^1.3.34"
    },
    "cloudflare": {
      "@cloudflare/workers-types": "^4.20251109.0"
    },
    "drizzle": {
      "drizzle-kit": "^0.31.6",
      "drizzle-orm": "^0.44.7",
      "drizzle-zod": "^0.8.3"
    },
    "orpc": {
      "@orpc/client": "^1.11.0",
      "@orpc/contract": "^1.11.0",
      "@orpc/openapi": "^1.11.0",
      "@orpc/openapi-client": "^1.11.0",
      "@orpc/server": "^1.11.0"
    }
  }
}
